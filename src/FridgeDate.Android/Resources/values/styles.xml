<?xml version="1.0" encoding="UTF-8" ?>
<resources>
  <!-- Inherit from the light Material Theme -->
  <style name="FridgeTheme" parent="android:Theme.Holo.Light">
    <!-- Override the app bar color -->
    <item name="android:colorPrimary">@color/primary</item>
    <!-- Override the color of UI controls -->
    <item name="android:colorAccent">@color/secondary</item>
    <item name="android:editTextColor">@color/dark</item>
    <item name="android:textColorPrimary">@color/dark</item>
    <item name="android:textColorSecondary">@color/dark</item>
    <item name="android:textColorTertiary">@color/dark</item>
    <item name="android:textColorPrimaryInverse">@color/dark</item>
    <item name="android:textColorSecondaryInverse">@color/dark</item>
    <item name="android:textColorTertiaryInverse">@color/dark</item>

  </style>
  <style name="button_text" >
    <item name="android:textColor" >#ffffff</item>
  </style>
</resources>